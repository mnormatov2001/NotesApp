<!DOCTYPE html>

<html lang="en">
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    
    <link rel="icon" href=@Url.Content("~/img/logo.svg") type="image/svg" />
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Inter:wght@100..900&display=swap" rel="stylesheet">

    <title>@ViewBag.Title</title>

    <link href=@Url.Content("~/css/bootstrap.min.css") rel="stylesheet" type="text/css" />
    <link href=@Url.Content("~/css/app.css") rel="stylesheet" type="text/css" />
</head>
<body class="inter-font">
    <div class="wrapper">
        @RenderBody()
        @{
            await Html.RenderPartialAsync("Footer");
        }
    </div>
    <script src=@Url.Content("~/js/bootstrap.bundle.min.js") type="text/javascript"></script>
</body>
</html>
